      "drupal/core-gettext": "~8.4",
      "drupal/core-graph": "~8.4",
      "drupal/core-http-foundation": "~8.4",
      "drupal/core-php-storage": "~8.4",
      "drupal/core-plugin": "~8.4",
      "drupal/core-proxy-builder": "~8.4",
