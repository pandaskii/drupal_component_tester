{
  "name": "mile23/drupal_component_tester",
  "description": "Run Drupal component tests in isolation.",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
      "drupal/core-annotation": "~8.4",
      "drupal/core-assertion": "~8.4",
